{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": ["@nuxt/types", "@types/node", "@nuxtjs/auth-next", "@nuxtjs/moment", "@nuxtjs/color-mode", "@nuxtjs/axios"]
  },
  "extends": "../../tsconfig.json",
  "exclude": ["node_modules", ".nuxt", "dist"]
}

